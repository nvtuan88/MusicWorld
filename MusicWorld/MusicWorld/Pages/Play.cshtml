@page
@model MusicWorld.Pages.PlayModel

<h2>Your Play List</h2>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var line in Model.Playlist.Lines)
        {
        <tr>
            <td class="text-center">@line.Musics.Name</td>
            <td class="text-center">@line.Musics.Url</td>
        </tr>
        }
    </tbody>
</table>
<div class="text-center">
    <a class="btn btn-primary" href="@Model.ReturnUrl">Continue shopping</a>
</div>